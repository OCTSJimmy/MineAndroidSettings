<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE:PREVIEW" />
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="question" pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <pattern pattern="\bsign\b.*" />
    <pattern useCustomColors="true" pattern="\bdone\b.*">
      <option name="FOREGROUND" value="8c8c8c" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="8c8c8c" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>